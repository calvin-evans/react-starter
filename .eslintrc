{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "standard",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-undef": 0,
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      2
    ],
    "import/first": 0,
    "no-extra-semi": "error",
    "no-debugger": 0,
    "no-console": 1,
    "react/jsx-uses-vars": 2,
    "react/jsx-uses-react": 2,
    "react/react-in-jsx-scope": 2,
    "react/prop-types": 0,
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "no-unused-vars": 1
  }
}
