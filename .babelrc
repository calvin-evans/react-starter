{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "last 2 versions",
          "safari >= 7"
        ]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      { "corejs": 2 }
    ],
    [
      "module-name-mapper",
      {
        "moduleNameMapper": {
          "\\.scss$": "<rootDir>/src/helpers/style-mock.js"
        }
      }
    ]
  ]
}
